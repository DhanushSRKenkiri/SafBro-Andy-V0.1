<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.SafBroLauncher" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:statusBarColor">@color/black</item>
    </style>

    <style name="Theme.SafBro" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:statusBarColor">@color/black</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.SafBro.BlackScreen">
        <item name="android:windowBackground">@color/black</item>
        <item name="android:statusBarColor">@color/black</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


</resources>