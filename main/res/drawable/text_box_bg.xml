<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background shape -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#00000000" /> <!-- Change to your desired background color -->
            <corners android:radius="8dp" /> <!-- Adjust corner radius as needed -->
        </shape>
    </item>
    <!-- Border -->
    <item android:right="2dp" android:left="2dp" android:top="2dp" android:bottom="2dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="#895443" />
        </shape>
    </item>
</layer-list>
